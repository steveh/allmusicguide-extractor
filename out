function loadVideo(currentImg) {
currentImg = counter % cnt
document.getElementById("video_link").onclick = function(){mv("http://www.allmusic.com/cg/amg.dll?p=amg&sql=56:*publishedID="+videos[currentImg][0])}
document.getElementById("video_screen").src = videos[currentImg][1];
document.getElementById("video_title").onclick = function(){mv("http://www.allmusic.com/cg/amg.dll?p=amg&sql=56:*publishedID="+videos[currentImg][0])}
document.getElementById("video_title").innerHTML = videos[currentImg][2];
document.getElementById("video_artist").innerHTML = videos[currentImg][3];
document.getElementById("video_artist").href = videos[currentImg][4];
}
function switchVideo(mode) {
    switch (mode) {
    case 1:
	    clearInterval(timerID)
	    counter = counter + 1
	    loadVideo()
	    timerID = setInterval("switchVideo(0);resumeSlide();", 60 * 1000)
	    break
    case -1:
	    clearInterval(timerID)
	    counter = counter - 1
	    loadVideo();
	    timerID = setInterval("switchVideo(0);resumeSlide();", 60 * 1000)
	    break
    case 0:
	    counter = counter + 1
	    loadVideo()
	break
}
}
function resumeSlide() {
    clearInterval(timerID);
    timerID = setInterval("switchVideo(0)", interval * 1000)
}
